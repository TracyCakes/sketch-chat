{
  "name": "sketch-chat",
  "description": "A Sketch plugin to chat in Sketch Cloud files",
  "version": "0.1.1",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-chat",
    "manifest": "src/manifest.json",
    "main": "sketch-chat.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build && cd backend && npm run build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "cd backend && NODE_ENV=dev npm install && cd .. && npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "@types/randomstring": "^1.1.6",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "mocha-js-delegate": "^0.2.0",
    "randomstring": "^1.1.5",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sketch-module-web-view": "^3.1.4",
    "socket.io-client": "^2.3.0"
  },
  "author": "Mathieu Dutour <mathieu@dutour.me>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathieudutour/sketch-chat.git"
  }
}
